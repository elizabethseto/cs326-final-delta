{
  "name": "wevote",
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "lint": "npx eslint ",
    "dev": "nodemon server.js"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "http-server": "^0.12.3",
    "nodemon": "^2.0.14"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-var": "error",
      "eqeqeq": "error",
      "semi": [
        "error",
        "always"
      ],
      "curly": [
        "warn",
        "all"
      ],
      "prefer-const": [
        "warn",
        {}
      ]
    }
  },
  "dependencies": {
    "uuid": "^8.3.2"
  },
  "description": "app",
  "version": "1.0.0",
  "main": "server.js",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizabethseto/cs326-final-delta.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elizabethseto/cs326-final-delta/issues"
  },
  "homepage": "https://github.com/elizabethseto/cs326-final-delta#readme"
}
